<div class="denomination-list row">
  <% var counter = 1 %>
  <% currencies[i][type].forEach(function(denomination) { %>
    <% if (!currencies[i]['range'] || ((!currencies[i]['range'][0] || currencies[i]['range'][0] <= Object.keys(denomination)[0]) && (!currencies[i]['range'][1] || currencies[i]['range'][1] >= Object.keys(denomination)[0]))) { %>
      <div class="denomination col span_1 item-<%= counter %>">
        <div class="<%= denomination[Object.keys(denomination)[0]] %> <%= type %>"><div class="color <%= denomination[Object.keys(denomination)[0]] %>"></div><div class="inner"><% if (!currencies[i]['after']) { %><span class="symbol"><%= currencies[i]['symbol'] %></span><% } %><%= valueFormatted(Object.keys(denomination)[0]) %><% if (currencies[i]['after']) { %><span class="symbol"><%= currencies[i]['symbol'] %></span><% } %><!-- (<%= currencies[i]['name'] %>) --></div>
        </div>
        <div class="subtitle"><% if (!currencies[j]['after']) { %><span class="symbol"><%= currencies[j]['symbol'] %></span><% } %><%= (Object.keys(denomination)[0] * currencies[i].worth[j]).toFixed(2) !== '0.00' ? (Object.keys(denomination)[0] * currencies[i].worth[j]).toFixed(2) : '0.01' %><% if (currencies[j]['after']) { %><span class="symbol"><%= currencies[j]['symbol'] %></span><% } %>
        </div>
      </div>
      <% counter++ %>
    <% } %>
  <% }) %>

  <div class="clear"></div>
</div>
